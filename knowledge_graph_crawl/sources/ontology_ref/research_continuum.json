{
  "nodes": [
    {
      "id": "A2",
      "label": "A PROPOSED FRAMEWORK TO INVESTIGATE BUILDING INFORMATION MODELLING THROUGH KNOWLEDGE ELICITATION AND VISUAL MODELS",
      "type": "Publication",
      "desc": "Bilal Succar, Willy Sher, Guillermo Aranda-Mena, Tony Williams"
    },
    {
      "id": "MD1",
      "label": "Six-Stage Methodology",
      "type": "Model",
      "desc": "A six-stage methodology to define, capture and represent BIM interactions through knowledge elicitation and visual knowledge models."
    },
    {
      "id": "MD2",
      "label": "BIM Maturity Levels",
      "type": "Model",
      "desc": "Represent the quality, predictability and variability within BIM stages"
    },
    {
      "id": "MD3",
      "label": "BIM Competencies",
      "type": "Model",
      "desc": "Represent incremental progressions towards and improvements within BIM stages"
    },
    {
      "id": "MD4",
      "label": "Organizational Scales",
      "type": "Model",
      "desc": "Represent the diversity of markets, disciplines and company sizes"
    },
    {
      "id": "MD5",
      "label": "Granularity Levels",
      "type": "Model",
      "desc": "Enable highly targeted yet flexible performance analyses ranging from informal self-assessment to high-detail, formal organizational audits"
    },
    {
      "id": "TR1",
      "label": "BIM Policy Node",
      "type": "Terminology",
      "desc": "The field of interaction generating research frameworks, standards and best practices for the purpose of safeguarding benefits and minimizing conflict between BIM stakeholders"
    },
    {
      "id": "TR2",
      "label": "BIM Process Node",
      "type": "Terminology",
      "desc": "The field of interaction between construction requirements, construction deliverables, organisational structures and operational communications for the purpose of generating and maintaining building information models."
    },
    {
      "id": "TR3",
      "label": "BIM Technology Node",
      "type": "Terminology",
      "desc": "The field of interaction between software, hardware and networking systems necessary to generate and maintain building information models."
    },
    {
      "id": "TR4",
      "label": "BIM Interactions",
      "type": "Terminology",
      "desc": "Push-pull knowledge transactions within and between BIM nodes"
    },
    {
      "id": "TR5",
      "label": "BIM Diffusion",
      "type": "Terminology",
      "desc": "diffusion represents the spread of the system/process across the organization. That is, the diffusion of a solution occurs after the solution has been adopted  (Peansupap & Walker, 2005) or what we term as the Point of Adoption  (PoA)."
    },
    {
      "id": "FR1",
      "label": "BIM Research Framework",
      "type": "Framework",
      "desc": "A prototypical framework and an investigative methodology to identify, capture and represent BIM interactions. Composed of three interlocking knowledge nodes and their push-pull interactions."
    },
    {
      "id": "CL1",
      "label": "Knowledge Visualisation",
      "type": "Classification",
      "desc": "Focuses on knowledge discovery, representation and augmentation without being limited to computer-assisted formats"
    },
    {
      "id": "CL2",
      "label": "Information Visualisation",
      "type": "Classification",
      "desc": "Computer-assisted visual processing to gain understanding of data relations"
    },
    {
      "id": "CL3",
      "label": "Mandates",
      "type": "Classification",
      "desc": "Documents which are prescriptive and dictated by an authority. Mandates identify what should be delivered and, in some cases, how, when, and by whom."
    },
    {
      "id": "CL4",
      "label": "Project Lifecycle Phases",
      "type": "Classification",
      "desc": "Construction projects pass through three major lifecycle phases: Design [D], Construction [C] and Operations [O]."
    },
    {
      "id": "CL5",
      "label": "BIM Steps",
      "type": "Classification",
      "desc": "Incremental evolutionary steps. Includes Technology Steps, Process Steps, and Policy Steps."
    },
    {
      "id": "CL6",
      "label": "Coupling Metrics",
      "type": "Classification",
      "desc": "Coupling Level (Clevel), Level of Accuracy (LoA), Representation Fidelity (RF), Degree of Automation (DoA), Degree of Transparency (DoT), Asset Scale"
    },
    {
      "id": "CL7",
      "label": "Coupling Enablers",
      "type": "Classification",
      "desc": "Hardware, Network, Software"
    },
    {
      "id": "CL8",
      "label": "Information Actions",
      "type": "Classification",
      "desc": "Clusters of granular information activities and tasks to be completed to deliver, use and reuse an asset."
    },
    {
      "id": "CL9",
      "label": "Information Sets",
      "type": "Classification",
      "desc": "Document/module types for collating asset lifecycle information."
    },
    {
      "id": "CL10",
      "label": "Information Tiers",
      "type": "Classification",
      "desc": "The information to be organized and shared through a Common Information Environment."
    },
    {
      "id": "TX1",
      "label": "BIM Ontology",
      "type": "Taxonomy",
      "desc": "A general BIM ontology generated through an initial seven-step approach. A high-level formal ontology of BIM interactions subdivided into lower level taxonomies and partonomies."
    },
    {
      "id": "MD6",
      "label": "Macro-Diffusion Responsibilities Model",
      "type": "Model",
      "desc": "Model about roles played by different stakeholder groups in facilitating diffusion within and across markets"
    },
    {
      "id": "TX2",
      "label": "BIM Knowledge Content Taxonomy",
      "type": "Taxonomy",
      "desc": "Clarifies and supports submitted papers (Appendix B)"
    },
    {
      "id": "FR2",
      "label": "Tri-Axial Framework",
      "type": "Framework",
      "desc": "Identifies Model Uses as the intended or expected model-based deliverables"
    },
    {
      "id": "FR3",
      "label": "Competency Framework",
      "type": "Framework",
      "desc": "Defines Model Use classes according to the nine topics within the Operation Competency Set"
    },
    {
      "id": "TL1",
      "label": "Visualisation",
      "type": "Tool",
      "desc": "Utilizes graphical means to explore, communicate or resolve logical problems"
    },
    {
      "id": "EP1",
      "label": "BIM Education is the shared responsibility of academia and industry",
      "type": "Model",
      "desc": "BIM Education is the shared responsibility of academia and industry"
    },
    {
      "id": "EP2",
      "label": "BIM Education addresses the requirements of current professionals (irrespective of formal qualification), future professionals (students) and their teachers/ trainers",
      "type": "Model",
      "desc": "BIM Education addresses the requirements of current professionals (irrespective of formal qualification), future professionals (students) and their teachers/ trainers"
    },
    {
      "id": "EP3",
      "label": "BIM Education encompasses all modes of BIM Learning (tertiary courses, industry workshops, online media and on-the-job training)",
      "type": "Model",
      "desc": "BIM Education encompasses all modes of BIM Learning (tertiary courses, industry workshops, online media and on-the-job training)"
    },
    {
      "id": "EP4",
      "label": "BIM Education ranges from spreading awareness to developing highly specialised skills",
      "type": "Model",
      "desc": "BIM Education ranges from spreading awareness to developing highly specialised skills"
    },
    {
      "id": "EP5",
      "label": "BIM Education should be made available to all those who need it in formats which are mindful of their respective disciplines, specialities, roles, education and experience levels",
      "type": "Model",
      "desc": "BIM Education should be made available to all those who need it in formats which are mindful of their respective disciplines, specialities, roles, education and experience levels"
    },
    {
      "id": "EP6",
      "label": "Collaborative BIM Education should be developed and delivered collaboratively",
      "type": "Model",
      "desc": "Collaborative BIM Education should be developed and delivered collaboratively"
    },
    {
      "id": "EP7",
      "label": "Every individual within the construction industry is a potential BIM Learner, and every BIM Learner is a potential BIM Learning Provider",
      "type": "Model",
      "desc": "Every individual within the construction industry is a potential BIM Learner, and every BIM Learner is a potential BIM Learning Provider"
    },
    {
      "id": "TL2",
      "label": "Metric 2: Distribution of NBPs",
      "type": "Tool",
      "desc": "Qualitative metric:  The distribution of NBPs across BIM Knowledge Content (BKC) labels and clusters."
    },
    {
      "id": "TL3",
      "label": "Metric 3: Relevance",
      "type": "Tool",
      "desc": "Qualitative metric: The relevance of each NBP across markets, measured using a five-level Relevance (R) index (R0 - R4)."
    },
    {
      "id": "A1",
      "label": "Building Information Modeling Analyzing Noteworthy Publications of Eight Countries Using a Knowledge Content Taxonomy",
      "type": "Publication",
      "desc": "A paper analyzing 57 noteworthy BIM publications from eight countries using a specialized taxonomy."
    }
  ],
